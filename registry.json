{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "@arach",
  "homepage": "https://registry.arach.dev",
  "items": [
    {
      "name": "terminal",
      "type": "registry:style",
      "title": "Terminal Theme",
      "description": "A sophisticated terminal-inspired theme with military-grade precision design and tactical UI patterns",
